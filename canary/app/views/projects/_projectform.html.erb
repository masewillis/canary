<%= form_for project_local do |f| %>
  <%= f.text_field :name, {placeholder: "project name"} %>
  <%= f.text_field :company_id, {placeholder: "company id"} %>
  <%= f.text_field :country_id, {placeholder: "country id"} %>
  <%= f.text_field :minefield_id, {placeholder: "minefield id"} %>
  <%= f.text_field :description, {placeholder: "description"} %>
  <%= f.hidden_field :company_id, value: company_id %>
  <%= f.hidden_field :country_id, value: country_id %>
  <%= f.hidden_field :minefield_id, value: minefield_id %>
  <%= f.submit %>
<% end %>
